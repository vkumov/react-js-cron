{
  "name": "@vkumov/react-js-cron-cui",
  "version": "1.0.2",
  "description": "A React cron editor with antd inspired by jqCron",
  "author": "Xavier Rutayisire <xavier.rutayisire@gmail.com> (https://github.com/xrutayisire/)",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "js",
    "cron",
    "crontab",
    "editor",
    "widget",
    "generator",
    "antd",
    "ant-design",
    "cronjob",
    "schedule",
    "parser"
  ],
  "homepage": "https://github.com/vkumov/react-js-cron#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkumov/react-js-cron.git"
  },
  "bugs": {
    "url": "https://github.com/vkumov/react-js-cron/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "del dist && yarn build:babel && yarn build:types",
    "build:babel": "concurrently \"yarn build:babel:esm\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "BABEL_ENV=cjs babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/cjs --presets=@babel/env",
    "build:babel:esm": "BABEL_ENV=esm babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/esm",
    "build:types": "tsc",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prettier": "prettier --write 'src/**/*.{ts?(x),css}'"
  },
  "peerDependencies": {
    "@vkumov/react-cui-2.0": "^4.5.12",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@babel/cli": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@types/history": "^2.0.51",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-modal": "^3.13.1",
    "@types/react-router": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@vkumov/react-cui-2.0": "^4.5.12",
    "antd": "^4.10.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "concurrently": "^5.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hoist-non-react-statics": "^3.3.2",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-modal": "^3.14.4",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  }
}
